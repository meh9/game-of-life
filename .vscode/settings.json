{
    "java.compile.nullAnalysis.mode": "automatic",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "python.formatting.provider": "black",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.variableTypes": true,
    "python.analysis.typeCheckingMode": "strict",
    "python.linting.pylintEnabled": true,
    "python.linting.mypyArgs": [
        "--strict",
        "--no-warn-unused-ignores"
    ],
    "python.linting.mypyEnabled": true,
    "python.linting.pydocstyleEnabled": true,
    "editor.formatOnSave": true,
    "java.saveActions.organizeImports": true,
    "java.cleanup.actionsOnSave": [
        "addOverride",
        "addDeprecated",
        "stringConcatToTextBlock",
        "invertEquals",
        "addFinalModifier",
        "instanceofPatternMatch",
        "lambdaExpression",
        "switchExpression",
        "tryWithResource"
    ],
    "python.linting.mypyCategorySeverity.error": "Warning",
    "python.linting.pycodestyleCategorySeverity.E": "Warning",
    "python.linting.pylintCategorySeverity.error": "Warning",
    "python.analysis.autoImportCompletions": true,
    "python.linting.pylintCategorySeverity.fatal": "Warning"
}